{
    "$schema": "http://json.schemastore.org/package.json",
    "name": "@bojanohomes/dashboard",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "dev": "concurrently 'pnpm dev:frontend' 'pnpm dev:backend'",
        "dev:backend": "pip3 install -r requirements.txt && python3 ./api/app.py",
        "dev:frontend": "next dev --turbo --hostname 0.0.0.0",
        "format": "pnpx prettier --write -- \"**/*.{js,jsx,mjs,cjs,ts,tsx,json,css,md}\" && pnpx sort-package-json",
        "lint": "next lint",
        "start": "next start"
    },
    "dependencies": {
        "@clerk/nextjs": "^6.2.1",
        "@hookform/resolvers": "^3.6.0",
        "@radix-ui/react-alert-dialog": "^1.0.5",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-select": "^2.0.0",
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-toast": "^1.1.5",
        "@stripe/stripe-js": "^3.5.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.378.0",
        "mongodb": "^6.7.0",
        "mongoose": "^8.4.1",
        "next": "^14.2.14",
        "next-cloudinary": "^6.6.2",
        "qs": "^6.12.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.51.5",
        "stripe": "^15.11.0",
        "svix": "^1.24.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/node": "^20",
        "@types/qs": "^6.9.15",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "concurrently": "^9.1.0",
        "eslint": "^8.57.1",
        "eslint-config-next": "14.2.3",
        "postcss": "^8",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "sort-package-json": "^2.10.1",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.4.14",
        "tailwindcss-animate": "^1.0.7"
    },
    "engines": {
        "node": ">=20.0"
    }
}
